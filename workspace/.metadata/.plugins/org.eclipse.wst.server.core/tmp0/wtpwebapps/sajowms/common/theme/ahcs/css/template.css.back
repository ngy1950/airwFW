#layer_pop3 .pop_wrap .close_btn a{ width:17px; height:17px; background-image:url(../images/btn/layer_pop2_btn_close.gif); background-position:0px 0px; background-repeat:no-repeat; text-indent:-99999px; padding:0px; margin:0px; float:right;}
#layer_pop2 .pop_bg,
#layer_pop3 .pop_bg{ width:100%; height:100%; background-color:#000; opacity:0.9; padding:0px; margin:0px; position:fixed; z-index:9;}


#layer_pop1 .h4_wrap .title p.btn_b > a,
#layer_pop2 .h4_wrap .title p.btn_b > a,
#layer_pop3 .h4_wrap .title p.btn_b > a{ background-color:#a9a9a9; border:1px solid #a4a4a4; padding:0px 15px;}


#grid_layer_pop{ width:100%; height:100%; padding:0px; margin:0px; position:fixed; z-index:9999; display:none;}
#grid_layer_pop .pop_wrap{ background-color:#fff; padding:0px; margin:0px 0px 0px 0px; display:inline-block; border-radius:2px; box-shadow:2px 2px 5px 0px #000; position:absolute; top:50%; left:50%; z-index:999;}
#grid_layer_pop .pop_wrap .pop_head{ width:100%; height:62px; background-color:#b50108; padding:0px; margin:0px; border-radius:2px 2px 0px 0px; border-bottom:3px solid #9f0006;}
#grid_layer_pop .pop_wrap .pop_head .title{ font-size:24px; color:#fff; line-height:62px; padding:0px; margin:0px 0px 0px 30px;}
#grid_layer_pop .pop_wrap .pop_body{ width:100%; max-height:800px; background-color:#fff; padding:56px 30px 0px 30px; margin:0px; border-radius:0px 0px 2px 2px; box-sizing:border-box; overflow-y:auto;}
#grid_layer_pop .pop_wrap .pop_body .h4_wrap{ margin-bottom:40px;}
#grid_layer_pop .pop_wrap .pop_body > .btn_b{ text-align:center; margin-bottom:40px;}
#grid_layer_pop .pop_wrap .pop_body > .btn_b a.on{ height:32px; background-color:#3c3d3e; line-height:32px; border:0px;}
#grid_layer_pop .pop_wrap .close_btn{ width:17px; height:17px; padding:0px; margin:0px; position:absolute; top:24px; right:30px;}
#grid_layer_pop .pop_wrap .close_btn a{ width:17px; height:17px; background-image:url(../images/btn/grid_control_pop_btn_close.gif); background-position:0px 0px; background-repeat:no-repeat; text-indent:-99999px; padding:0px; margin:0px; float:right;}
#grid_layer_pop .pop_bg{ width:100%; height:100%; background-color:#000; opacity:0.9; padding:0px; margin:0px; position:fixed; z-index:9;}
