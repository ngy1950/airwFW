.divide_wrpa.n3 .n1{ padding:0px 10px 0px 0px; margin:0px; box-sizing:border-box; float:left;}
.divide_wrpa.n3 .n2{ padding:0px 10px 0px 10px; margin:0px; box-sizing:border-box; float:left;}
.divide_wrpa.n3 .n3{ padding:0px 0px 0px 10px; margin:0px; box-sizing:border-box; float:left;}
