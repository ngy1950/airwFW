/*그리드 시작*/
.grid_scroll{ width:100%; padding:0px; margin:0px 0px 50px 0px; position:relative;}		

.grid_scroll > .grid_head{ width:100%; padding:0px; margin:0px; overflow:hidden;}
.grid_scroll > .grid_head > table{ width:0px; min-width:100%; background-color:#fff; font-family:dotum; padding:0px; margin:0px; border-top:2px solid #d22128; table-layout:fixed; border-collapse:separate; border-spacing:0px;}
