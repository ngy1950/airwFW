p.btn_b{ text-align:right; line-height:32px;}
p.btn_b > a{ min-width:41px; height:30px; background-color:#939393; font-size:12px; text-align:center; color:#fff; line-height:30px; padding:0px 12px; margin:0px; display:inline-block; border:1px solid #848484; border-radius:2px;}
