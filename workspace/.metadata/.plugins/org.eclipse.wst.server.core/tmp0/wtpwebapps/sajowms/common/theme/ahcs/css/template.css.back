span.search_box > .btn{ width:30px; height:30px; background-image:url(../images/btn/btn_icon_search.gif); background-position:center center; background-repeat:no-repeat; line-height:30px; text-indent:-99999px; padding:0px; margin:1px 0px 1px -31px; overflow:hidden; display:block; float:left;}

