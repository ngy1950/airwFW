CREATE TABLE SYSCOMMPOP(
	COMMPOPID	VARCHAR2(10)	PRIMARY	KEY,
	CPOPLABEL	VARCHAR2(10)	,
	CPOPNAME	VARCHAR2(50)	,
	VIEWTNAME	VARCHAR2(30)	,
	CPOPDESC	VARCHAR2(50)	,
	CPOPTYPE	VARCHAR2(10)	,
	EXETYPE		CHAR(1)			,
	SIZEW		NUMBER()		DEFAULT 800,
	SIZEH		NUMBER()		DEFAULT 900,
	
	CREATEUSER	VARCHAR2(10)	,
	CREATEDATE	DATE			,
	UPDATEUSER	VARCHAR2(10)	,
	UPDATEDATE	DATE			,
	DELETEYN	CHAR(1)			DEFAULT 'N'
);

EXETYPE = V - 화면 로드 시 자동 조회
SIZEW - 팝업창 넓이
SIZEH - 팝업창 높이

CREATE TABLE SYSCPOPITEM(
	COMMPOPID	VARCHAR2(10)	NOT	NULL,
	CPOPITEMID 	VARCHAR2(10)	NOT	NULL,
	CPOPITLABEL	VARCHAR2(10)	,
	CPOPITNAME	VARCHAR2(50)	,
	ITEMTYPE	VARCHAR2(10)	,
	SEARCHTYPE	VARCHAR2(10)	,
	SFORMAT		VARCHAR2(10)	,
	SOPTION		VARCHAR2(10)	,
	SDEFAULT	VARCHAR2(50)	,
	SREQUIRED	CHAR(1)			,
	GWIDTH		NUMBER			,
	RETURNCOL	CHAR(1)			,
	SORTORDER	NUMBER			,
	
	CREATEUSER	VARCHAR2(10)	,
	CREATEDATE	DATE			,
	UPDATEUSER	VARCHAR2(10)	,
	UPDATEDATE	DATE			,
	DELETEYN	CHAR(1)			DEFAULT 'N',
	
	CONSTRAINTS SYSCPOPITEM_PK PRIMARY KEY(COMMPOPID, CPOPITEMID)
);

ITEMTYPE
 - SEARCH : 조회조건에만 표시
 - GRID : 그리드에만 표시

RETURNCOL
 - V : 공통팝업 리턴 컬럼 value
 - T : 공통팝업 리턴 컬럼 text
 
SEARCHTYPE
 - CHECK : checkbox
 - INPUT : input
 - LIKE : input like
 - MULTI : range
 - SELECT : select(common code)
 - HIDDEN : hidden param
 - COMBO : select(module command sql)

SFORMAT
 - DATE : UIFormat='C N'
 - UPPER : UIFormat='U'
 - READONLY : 읽기전용


SOPTION
 - INPUT,MULTI : common popup id
 - SELECT : common combo id

SREQUIRED : validate='required'

SDEFAULT
 - 기본값
 - SES_USER_COMPANY : 세션의 회사 코드값 자동셋팅
 - MODULE,COMMAND :  SEARCHTYPE COMBO 인 경우 sql

GWIDTH
 - grid col width