.skip{
	position:absolute;
	top:-100%;
	left:-100%;
	width:90px;
	height:50px;
	background: url("/common/theme/webdek/images/login/skip.png") no-repeat center center;
	background-size:100% auto;
    transform: translate(-50%, -58%);
	cursor: pointer;
}

@keyframes slideDown {
  from {
    height:0;
  }
  to {
    height: 100%;
  }
}

@keyframes slideRight {
  from {
    width:0;
  }
  to {
    width: 100%;
  }
}

@keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}

