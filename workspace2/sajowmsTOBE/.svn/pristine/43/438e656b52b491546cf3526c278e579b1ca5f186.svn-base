@charset "utf-8";
@import "reset.css"; /* CSS 초기화 */

*{box-sizing:border-box;}

html,body {
  height: 100%;
  width: 100%;
  overflow: hidden;
}

body.GCLC {
	background: url(/common/theme/webdek/images/login/intro_bg.png) repeat center center; 
	background-size: cover;
}
body.GCCL {
	background: url(/common/theme/webdek/images/login/intro_bg_GCCL.png) repeat center center; 
	background-size: cover;
}

.intro_text{
	width:500px;
	height:480px;
	position: relative;
	top:50%;
	left:50%;
	transform: translate(-50%, -50%);
	text-align:center;
	color: #fff;
 /*  animation: name duration timing-function delay iteration-count direction fill-mode; */
	animation : fadeIn .5s linear 2s 1 reverse forwards;
}

.intro_text .text_Wrap{
	overflow: hidden;
	height: 0;
	
 /*  animation: name duration timing-function delay iteration-count direction fill-mode; */
	animation : slideDown 1s linear 0s 1 normal forwards;
}


.text_Wrap>*{
	display: block;
	border-bottom: 3px solid #fff;
	font-size: 100px;
	line-height: 150px;
	
}
.intro_text .text_Wrap b:LAST-CHILD {
	display: inline-block;
}

.menu{
	width: 700px;
	height: 600px;
	position: absolute;
	top: 50%;
	left: 50%;
	opacity :0;
	transform: translate(-50%, -50%);
 /*  animation: name duration timing-function delay iteration-count direction fill-mode; */
	animation : fadeIn .5s linear 3s 1 normal forwards;
}

.menu>div{
	position: absolute; 
	top: 0;
	left: 0;
    width: 100%;
    height: 100%;
}

.info h2{
	color: #327b33;
	font-size: 30px;
	text-align: center;
}
.info h2 .username{
	font-weight: bold;
	border-bottom: 2px solid #1b9b21;
}

.GCCL .info h2{
	color: #fff;
}
.GCCL .info h2 .username{
	border-bottom: 2px solid #fff;
}

.info .logoDiv{
	height: 350px;
	width: 350px;
	border-radius : 50%;
	position: absolute;
	top : 10%;
	left: 50%;
	transform: translateX(-50%);
	background-color: #fdedb3;
}
.GCCL .info .logoDiv{
	background-color: #e8eef2;
}

.info .logoDiv .logo{
	width: 70%;
    height: 70%;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
	opacity : 0;
 /*  animation: name duration timing-function delay iteration-count direction fill-mode; */
	animation : fadeIn .1s linear 3.7s 1 normal forwards;
}

.GCLC .info .logoDiv .logo{
	background: url("/common/theme/webdek/images/login/G-HUB_LOGO.png") no-repeat center center;
	background-size: 100% auto;
}

.GCCL .info .logoDiv .logo{
	background: url("/common/theme/webdek/images/login/GHUB_LOGO_b.png") no-repeat center center;
	background-size: 100% auto;
}

body .menu_Wrap{
	height: 100%;
	width: 0;
	overflow: hidden;
	animation : slideRight 1s linear 4s 1 normal forwards;
}

.menu_Wrap .menuBar{
	height: 390px;
	width: 410px;
	background: url(/common/theme/webdek/images/login/menuBar.png) no-repeat; 
	background-size : 100% 100%;
	position: absolute;
	top : 10%;
	left: 50%;
	transform: translateX(-50%);
	opacity: 0.7;
}

.menu_Wrap .menuPoint{
	width: 20px;
	height: 20px;
	border-radius : 50%;
	background-color: #f9f4bf;
	position: absolute;
} 

.GCCL .menu_Wrap .menuPoint{
	background-color: #fff;
}

.menu .menu_Wrap .menuList{
	height: 100%;
	width: 0;
	overflow: hidden;
	/*  animation: name duration timing-function delay iteration-count direction fill-mode; */
	animation : slideRight 1s linear 4s 1 normal forwards;
}

.menu_Wrap .menuList .menuDiv .menuName{
	position:absolute;
	top:103%;
	left:50%;
	transform: translateX(-50%);
	text-align: center;
	width: 100%;;
}

.menu_Wrap .menuList .menuDiv.focus{
	color: green;
}

.menu_Wrap .menuList .menuDiv{
	width: 100px;
	height: 100px;
	border-radius : 50%;
	position: absolute;
	color: #fdedb3;
	cursor: pointer;
    font-size: 14px;
    font-weight: bold;
}

.GCCL .menu_Wrap .menuList .menuDiv{
	color: #e8eef2;
    opacity:0.7;
}
.GCCL .menu_Wrap .menuList .menuDiv.focus{
	color: #fff;
}

