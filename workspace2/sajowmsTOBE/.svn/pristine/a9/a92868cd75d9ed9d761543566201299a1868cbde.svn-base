.login_btn .btn2{width:145px; height:45px; background:#f14f4f; border:#f14f4f; border-radius:5px; float:right; cursor:pointer; }

.login_btn a:hover{background:#fff}
.login_btn li{float:left; color:#fff; font-size:15px; font-weight:bold;}
.login_btn img{width:25px; margin:8px 0 0 35px;}

.login_btn .btn_txt{margin:12px 0 0 5px}

.login_btn .btn2 li{ margin:12px 0 0 50px}
.login_txt{max-width:330px; margin:80px auto; color:#999; font-size:13px; }
