<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!-- page transform -->
<div class="pageTransformArea" id="pageTransform">
	<div class="pageTransform">
		<div class="pageTransImg"></div>
	</div>
</div>
<!-- layerPop -->
<div class="mobilePopArea">
	<div class="modalWrap" id="alert">
		<div class="modal">
			<div class="modalContent">
				<div class="text">
				</div>
				<div class="button">
					<button class="confirm btnWid1 btnBod1">
						<span CL="BTN_CONFIRM"></span>
					</button>
				</div>
			</div>
		</div>
	</div>
	<div class="modalWrap" id="confirm">
		<div class="modal">
			<div class="modalContent">
				<div class="text">
				</div>
				<div class="button">
					<button class="cancel btnWid2 btnBod2 btnAlinL">
						<span CL="BTN_CANCEL"></span>
					</button>
					<button class="confirm btnWid2 btnBod1 btnAlinR">
						<span CL="BTN_CONFIRM"></span>
					</button>
				</div>
			</div>
		</div>
	</div>
	<div class="toastWrap" id="toast">
		<div class="toast">
			<div class="text">
				<p class="img"></p>
				<p class="txt"></p>
			</div>
		</div>
	</div>
	
	<div class="datePickerWrap" id="datePicker">
		<div class="datePickerContent">
			<div class="datePicker">
				<div class="title">
					<ul>
						<li class="left1"></li>
						<li></li>
						<li class="left2"></li>
					</ul>
				</div>
				<div class="date left1">
					<p class="holderTop"></p>
					<p class="holderBottom"></p>
					<div class="year">
						<ul></ul>
					</div>
				</div>
				<div class="date">
					<p class="holderTop"></p>
					<p class="holderBottom"></p>
					<div class="month">
						<ul></ul>
					</div>
				</div>
				<div class="date left2">
					<p class="holderTop"></p>
					<p class="holderBottom"></p>
					<div class="day">
						<ul></ul>
					</div>
				</div>
				<div class="button">
					<button class="confim"><p CL="BTN_CONFIRM"></p></button>
					<button class="cancel"><p CL="BTN_CLOSE"></p></button>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- virtual keyboarad area-->
<div class="virtualKeypadWrap">
	<div class="virtualKeypad">
		<div class="virtualKeypadContent">
			<div class="topArea">
				<button class="close">Close</button>
			</div>
			<table>
				<tr>
					<td>
						<button>1</button>
					</td>
					<td>
						<button>2</button>
					</td>
					<td>
						<button>3</button>
					</td>
					<td>
						<button class="delete bg1">
							<img class="delete" src="/pda/img/ico/ico_btn_del.png"/>
						</button>
					</td>
				</tr>
				<tr>
					<td>
						<button>4</button>
					</td>
					<td>
						<button>5</button>
					</td>
					<td>
						<button>6</button>
					</td>
					<td style="width: 30%;">
						<button class="clear bg1" style="font-size: 90%;">Clear</button>
					</td>
				</tr>
				<tr>
					<td>
						<button>7</button>
					</td>
					<td>
						<button>8</button>
					</td>
					<td>
						<button>9</button>
					</td>
					<td rowspan="2">
						<button class="enter bg1">
							<img class="enter" src="/pda/img/ico/ico_btn_enter.png"/>
						</button>
					</td>
				</tr>
				<tr>
					<td></td>
					<td>
						<button>0</button>
					</td>
					<td></td>
				</tr>
			</table>
		</div>	
	</div>	
</div>
